{
  "name": "DICOM anonymization",
  "authors": "Rudolph Pienaar<rudolph.pienaar@childrens.harvard.edu",
  "category": "Medical Imaging",
  "description": "Anonymize an input directory of DICOM files -- also create two branches side branches of extracted DICOM tag info.",
  "locked":	false,
  "plugin_tree": [
    {
      "plugin_name": "pl-simpledsapp",
      "plugin_version": "2.1.0",
      "previous_index": null
    },
    {
      "plugin_name": "pl-pfdicom_tagextract",
      "plugin_version": "3.1.3",
      "previous_index": 0,
      "plugin_parameter_defaults": [
        {
            "name":         "extension",
            "default":      "dcm"
        },
        {
            "name":         "outputFileStem",
            "default":      "originalTags"
        },
        {
            "name":         "imageFile",
            "default":      "m:%_nospc|-_ProtocolName.jpg"
        },
        {
            "name":         "imageScale",
            "default":      "3:none"
        },
        {
            "name":         "outputFileType",
            "default":      "raw,html"
        }
      ]
    },
    {
      "plugin_name": "pl-pfdicom_tagsub",
      "plugin_version": "3.2.4",
      "previous_index": 0,
      "plugin_parameter_defaults": [
        {
            "name":         "extension",
            "default":      ".dcm"
        },
        {
            "name":         "tagInfo",
            "default":      "'PatientName,%_name|patientID_PatientName ++ PatientID,%_md5|7_PatientID ++ AccessionNumber,%_md5|8_AccessionNumber ++ PatientBirthDate,%_strmsk|******01_PatientBirthDate ++ re:.*hysician,%_md5|4_#tag ++ re:.*stitution,#tag ++ re:.*ddress,#tag'"
        },
        {
            "name":         "splitToken",
            "default":      "++"
        },
        {
            "name":         "splitKeyValue",
            "default":      ","
        }
      ]
    },
    {
        "plugin_name": "pl-pfdicom_tagextract",
        "plugin_version": "3.1.3",
        "previous_index": 2,
        "plugin_parameter_defaults": [
            {
                "name":         "extension",
                "default":      "dcm"
            },
            {
                "name":         "outputFileStem",
                "default":      "anonymizedTags"
            },
            {
                "name":         "imageFile",
                "default":      "m:%_nospc|-_ProtocolName.jpg"
            },
            {
                "name":         "imageScale",
                "default":      "3:none"
            },
            {
                "name":         "outputFileType",
                "default":      "raw,json,html,dict,col,csv"
            }        
        ]
    }
  ]
}

